
@{
    ViewBag.Title = "_Test";
}
<!DOCTYPE HTML>
<html>
<head>
    <title>Easy-Trip</title>
    <meta http-equiv="Content-Type" content="text/html; charset=utf-8" />
    <link rel="stylesheet" href="~/web/css/style.css" type="text/css" media="all" />
    <meta name="viewport" content="width=device-width, initial-scale=1, maximum-scale=1">
    <link href="~/web/css/slider.css" rel="stylesheet" type="text/css" media="all" />
    <script src="~/web/js/jquery.min.js"></script>
    <script src="~/web/js/responsiveslides.min.js"></script>

    <script>
        // You can also use "$(window).load(function() {"
        $(function () {
            // Slideshow 1
            $("#slider1").responsiveSlides({
                auto: true,
                nav: true,
                speed: 500,
                namespace: "callbacks",
            });
        });
    </script>
</head>
<body>
    <div class="header">
        <div class="wrap">
            <div class="logo"><a href="/Default/Index"><img src="/web/images/logo.png" alt="" /></a></div>
            <div class="con-right">
                <div class="grid1-l-img">
                    <img src="/web/images/contact.png" alt="">
                </div>
                <div class="grid1-l-desc">
                    <p>WhatsaApp : +91 00000-55555</p>
                </div>
                <div class="clear"></div>
            </div>
            <div class="clear"></div>
        </div>
    </div>
    <div class="header-bottom mt-5">
        <div class="wrap">
            <ul id="nav">
                <li><a href="/Default/Index">Ana Sayfa</a></li>
                <li><a class="hsubs" href="/About/Index">Hakkımızda</a></li>
                <li><a class="hsubs" href="/Galeri/Index">Galeri</a></li>
                <li><a class="hsubs" href="/Blog/Index">Blogum</a></li>
                <li><a href="/Contact/Index">İletişim</a></li>
            </ul>
        </div>
    </div>
    <div class"content">
        @RenderBody()
    </div>
    <div class="footer-bottom">
        <div class="wrap">
            <div class="copy">
                <p>&copy; 2023 Easy-Trip. Her hakkı saklıdır | Tasarım by Çetin Yazıcı <a href="http://w3layouts.com">W3Layouts</a></p>
            </div>
        </div>
    </div>
</body>
</html>




